generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id  Int @default(autoincrement()) @unique @id
  email String @unique
  name String
  posts Post[]
}
model Post{
  id Int @default(autoincrement()) @unique @id
  title String
  content String
  publicado Boolean @default(false)
  author User? @relation(fields: [authorId], references: [id])
  authorId Int
}